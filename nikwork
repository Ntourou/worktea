# Ονόματα εργαζομένων
ονόματα = ["Γιώργος", "Μαρία", "Κώστας", "Ελένη", "Πέτρος"]

# Ημέρες εργασίας
ημέρες = ["Δευτέρα", "Τρίτη", "Τετάρτη", "Πέμπτη", "Παρασκευή"]

# Εκτύπωση ονομάτων και ημερών εργασίας
for όνομα in ονόματα:
    for ημέρα in ημέρες:
        print(f"{όνομα} - {ημέρα}")

# Εκτύπωση μηνύματος για ρεπό
print("Σάββατο και Κυριακή ρεπό!")
